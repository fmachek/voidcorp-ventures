config_version=5

[application]

config/name="Voidcorp Ventures"
config/description="A strategy/tycoon game set in space, where you work your way toward intergalactic travel."
config/version="v1.1.1"
run/main_scene="res://scenes/Game.tscn"
config/features=PackedStringArray("4.3", "Forward Plus")
boot_splash/bg_color=Color(0.0204414, 4.99357e-07, 0.194639, 1)
boot_splash/show_image=false
boot_splash/image="res://assets/icon.png"
config/icon="res://assets/icon.png"
config/windows_native_icon="res://assets/icon.ico"
run/show_image=true

[autoload]

GameManager="*res://scripts/gamelogic/game_manager.gd"
